        <step type="prompt" delay="60" prompt="PROMPT_STEP_LO2_REFLECTION">Prompt_Step_LO2_Reflection</step>    	<!-- 60 -->
    </stage> 
    
    <stage name="StageLO3" type="other" delay="0">
