<?xml version="1.0" encoding="UTF-8"?>
<prompts>
	<prompt id="INTRODUCE">
		<text>It's me! :-)</text>
	</prompt>
	<prompt strategy="5" id="DO_CONCLUDE_SOCIAL">
		<text>It was fun working with you all. :-)</text>
	</prompt>
	<prompt strategy="2e" id="ENCOURAGE_GROUP">
		<text>I can't do this on my own :-)</text>
		<text>...is anyone working?</text>
		<text>Let's stick to this.</text>
		<text>Team -- I need to see more contributions from all of you!</text>
	</prompt>
	<prompt strategy="7" id="APOLOGIZE">
		<text>Sorry... Must be my mistake</text>
		<text>I wasn't paying attention! Sorry...</text>
		<text>Hmmm.. My bad, sorry guys</text>
		<text>Did I miss something?</text>
	</prompt>
	<prompt strategy="2d" id="ENCOURAGE_INDIVIDUAL">
		<text>Do you have any thoughts, [STUDENT]?</text>
		<text>What do you think, [STUDENT]?</text>
		<text>hey [STUDENT], are you still with us?</text>
	</prompt>
	<prompt strategy="2e" id="ENCOURAGE_GROUP_DIALOG">
		<text>It's no fun when nobody's talking...</text>
		<text>It would be great to have everyone in on this discussion :-)</text>
		<text>Hey, team... you gotta talk to each other.</text>
	</prompt>
	<prompt strategy="2c" id="PRAISE">
		<text>This is a great discussion!</text>
		<text>It's good to have you in my group :-)</text>
		<text>Thanks for taking the time to explain things to me!</text>
		<text>Y'all are awesome.</text>
	</prompt>
	<prompt strategy="2a" id="BE_PROTECTIVE">
		<text>hey, now.</text>
		<text>:-/</text>
		<text>We've got to work as a team!</text>
	</prompt>
	<prompt strategy="2b" id="REASSURE">
		<text>You can figure this out as a team!</text>
		<text>You're all in this togetherâ€¦ help each other!</text>
		<text>One of the great things about teamwork is helping each other.
		</text>
	</prompt>
	<prompt strategy="2f" id="SMILE">
		<text>:-)</text>
	</prompt>
	<prompt strategy="2g" id="PARTICIPATE_HAPPY">
		<text>I'm glad to be working with this team!</text>
		<text>We're a great team :-)</text>
		<text>Go team :-)</text>
	</prompt>
	<prompt strategy="4a" id="SHOW_ATTENTION_ENCOURAGEMENT">
		<text>Okay.</text>
		<text>Go on...</text>
		<text>Good</text>
		<text>Sounds good</text>
	</prompt>
	<prompt strategy="4b" id="SHOW_COMPREHENSION_APPROVAL">
		<text>Ok</text>
		<text>... ok</text>
		<text>... okay</text>
		<text>gotcha.</text>
		<text>Alright</text>
		<text>Got it</text>
	</prompt>
	<prompt strategy="6" id="BE_ANTAGONIST">
		<text>:-(</text>
		<text>Let's get serious about the assignment.</text>
		<text>...I don't think we'll finish like this.</text>
	</prompt>
	<prompt strategy="none" id="RANDOM_SOCIAL">
		<text>Let's keep going.</text>
		<text>Everything ok?</text>
		<text>Things seem to be going well...</text>
		<text>...</text>
		<text>This is the best team ever :-)</text>
	</prompt>
</prompts>